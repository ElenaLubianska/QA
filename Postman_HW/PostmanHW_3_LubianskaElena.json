							"var salary_after_12_months = salaryRequest * 2.9;\r",
							"var salary_after_6_months = salaryRequest * 2;\r",
							"\r",
							"pm.test(\"Property 'u_salary_1.5_year' eql 'salary' * 4\", () => {\r",
							"    pm.expect(jsonData.person.u_salary_1_5_year).to.eql(salary_1_5_year);\r",
							"});\r",
							"\r",
							"pm.test(\"Property 'qa_salary_after_12_months' eql 'salary' * 2.9\", () => {\r",
							"    pm.expect(jsonData.qa_salary_after_12_months).to.eql(salary_after_12_months);\r",
							"});\r",
							"\r",
							"pm.test(\"Property 'qa_salary_after_6_months' eql 'salary' * 2\", () => {\r",
							"    pm.expect(jsonData.qa_salary_after_6_months).to.eql(salary_after_6_months);\r",
							"});\r",
							"\r",
							"var salary_1_5year_resp;\r",
							"postman.setEnvironmentVariable(salary_1_5year_resp,jsonData.person.u_salary_1_5_year);"
